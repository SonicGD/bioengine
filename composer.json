{
  "name": "sonicgd/bioengine",
  "description": "BioEngine",
  "keywords": [
    "bioengine",
    "cms",
    "yii2"
  ],
  "homepage": "https://github.com/sonicgd/bioengine",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/sonicgd/bioengine/issues?state=open",
    "source": "https://github.com/sonicgd/bioengine"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.5",
    "yiisoft/yii2-bootstrap": "~2.0.5",
    "yiisoft/yii2-swiftmailer": "~2.0.5",
    "almasaeed2010/adminlte": "1.*",
    "2amigos/yii2-ckeditor-widget": "~1.0.4",
    "kartik-v/yii2-widget-select2": "1.*",
    "kartik-v/yii2-widget-fileinput": "1.*",
    "haslv/ipbwi": "1.0.*@dev",
    "bower-asset/phoenix": "*",
    "bower-asset/components-font-awesome": "*",
    "nao-pon/elfinder-nightly": "2.1.*@dev",
    "yiisoft/yii2-jui": "2.*",
    "bower-asset/jquery-ui": "~1.11@dev",
    "yiisoft/yii2-imagine": "^2.0@dev",
    "samdark/sitemap": "2.0.*",
    "sitkoru/yii2-ar-cache": "dev-cacheops",
    "yiisoft/yii2-redis": "dev-hhvm-compat as 2.0.3"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "dev-master"
  },
  "suggest": {
    "codeception/codeception": "Codeception, 2.0.* is currently works well with Yii.",
    "codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
    "codeception/verify": "BDD Assertions for PHPUnit and Codeception",
    "yiisoft/yii2-faker": "Fixtures generator for Yii2 based on Faker lib"
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::setPermission"
    ],
    "post-update-cmd": [
    ]
  },
  "extra": {
    "writable": [
      "backend/runtime",
      "backend/web/assets",
      "frontend/runtime",
      "frontend/web/assets"
    ],
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-4": {
      "bioengine\\": ""
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:SonicGD/yii2-redis.git"
    }
  ]
}
