{
  "name": "sonicgd/bioengine",
  "description": "BioEngine",
  "keywords": [
    "bioengine",
    "cms",
    "yii2"
  ],
  "homepage": "https://github.com/sonicgd/bioengine",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/sonicgd/bioengine/issues?state=open",
    "source": "https://github.com/sonicgd/bioengine"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "dev-master",
    "yiisoft/yii2-bootstrap": "dev-master",
    "yiisoft/yii2-swiftmailer": "dev-master",
    "almasaeed2010/adminlte": "dev-master",
    "2amigos/yii2-ckeditor-widget": "*",
    "kartik-v/yii2-widget-select2": "*",
    "kartik-v/yii2-widget-fileinput": "*",
    "haslv/ipbwi": "1.0.*@dev",
    "bower-asset/phoenix": "*",
    "bower-asset/components-font-awesome": "*",
    "studio-42/elfinder": "dev-jake-fixes"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "dev-master"
  },
  "suggest": {
    "codeception/codeception": "Codeception, 2.0.* is currently works well with Yii.",
    "codeception/specify": "BDD style code blocks for PHPUnit and Codeception",
    "codeception/verify": "BDD Assertions for PHPUnit and Codeception",
    "yiisoft/yii2-faker": "Fixtures generator for Yii2 based on Faker lib"
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::setPermission"
    ],
    "post-update-cmd": [
      "cd vendor/studio-42/elfinder && npm install uglify-js csso && mkdir -p build && jake -C ./build elfinder"
    ]
  },
  "config": {
    "github-oauth": {
      "github.com": "88c745eb265bb64d95995edfaaf4ae220aa741bb"
    },
    "process-timeout": 1800
  },
  "extra": {
    "writable": [
      "backend/runtime",
      "backend/web/assets",
      "frontend/runtime",
      "frontend/web/assets"
    ],
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-4": {
      "bioengine\\": ""
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:sonicgd/elFinder.git"
    }
  ]
}
